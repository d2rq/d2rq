    :schema <schemas/table1-iri.sql>;
    :triple (ex:s ex:p ex:1).

<mappings/iri-template-relative.ttl> a :R2RMLProcessorTestCase;
	:schema <schemas/table1.sql>;
	:triple (ex:s ex:p <http://example.com/path/1>).

<mappings/iri-template-absolute.ttl> a :R2RMLProcessorTestCase;
	:schema <schemas/table1.sql>;
	:triple (ex:s ex:p <http://notbase.example.com/1>).

<mappings/ref-object-map-no-joins1.ttl> a :R2RMLProcessorTestCase;
	:schema <schemas/table1.sql>;
	:triple (ex:s ex:p ex:s).

<mappings/ref-object-map-no-joins2.ttl> a :R2RMLProcessorTestCase;
	:schema <schemas/table1.sql>;
	:triple (ex:1 ex:p ex:1).

<mappings/ref-object-map-join-different-table.ttl> a :R2RMLProcessorTestCase;
    :schema <schemas/two-tables.sql>;
    :triple (ex:s1 ex:p ex:o11).

<mappings/ref-object-map-join-same-table.ttl> a :R2RMLProcessorTestCase;
    :schema <schemas/same-table-ref.sql>;
    :triple (ex:2 ex:p ex:1).

<mappings/ref-object-map-two-joins.ttl> a :R2RMLProcessorTestCase;
    :schema <schemas/two-tables-multicol.sql>;
    :triple (ex:s1 ex:p ex:o10_1), (ex:s2 ex:p ex:o10_2), (ex:s3 ex:p ex:o11_1).

<mappings/ref-object-map-join-same-view.ttl> a :R2RMLProcessorTestCase;
    :schema <schemas/same-table-ref.sql>;
    :triple (ex:2 ex:p ex:1).


