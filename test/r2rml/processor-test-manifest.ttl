    :schema <schemas/table1.sql>;
    :triple (ex:s rdf:type ex:C1), (ex:s rdf:type ex:C2).

<mappings/o-column-literal.ttl> a :R2RMLProcessorTestCase;
    :schema <schemas/table1.sql>;
