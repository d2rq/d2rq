<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss} %-5p %-20c{1} :: %m%n</pattern>
    </encoder>
  </appender>
  <logger name="de.fuberlin.wiwiss.d2rq.sql.QueryExecutionIterator" level="ALL"/>
  <logger name="org.joseki" level="INFO"/>
  <logger name="com.hp.hpl.jena" level="WARN"/>
  <logger name="de.fuberlin.wiwiss.d2rq.server.velocity" level="WARN"/>
  <root level="INFO">
    <appender-ref ref="stdout"/>
  </root>
</configuration>